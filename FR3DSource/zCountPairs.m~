
File = zAddNTData('1s72',2);


fprintf('FileID cWW only - cWW and ncWW - 1 ncWW - 2 ncWW - 3 ncWW - more ncWW - stack only - nothing\n');

for f = 1:length(File),
  A = 0;
  B = 0;
  C = 0;
  D = zeros(1,10);
  E = fix(abs(File.Edge));
  cWW   = sum(E == 1, 2);
  ncWW  = sum((1 < E) .* (E < 15),2);
  stack = sum((19 < E) .* (E < 24),2);
  for n = 1:length(File.NT),
    if (cWW(n) > 0) && (ncWW(n) == 0),
      A = A + 1;
    elseif (cWW(n) > 0) && (ncWW(n) > 0),
      B = B + 1;
    elseif (cWW(n) == 0) && (ncWW(n) == 0) && (stack(n) > 0),
      C = C + 1;
    elseif (cWW(n) == 0) && (ncWW(n) == 0) && (stack(n) == 0),
      C = C + 1;
    elseif (cWW(n) == 0) && (ncWW(n) > 0),
      D(ncWW(n)) = D(ncWW(n)) + 1;
    end
  end
  fprintf('%s    %8d %8d %8d %8d %8d %8d\n', File(f).Filename, 

end

